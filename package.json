{
  "name": "graphwalker",
  "version": "00.00.02",
  "license": "MIT",
  "description": "Graph traversal tool for the web browser",
  "repository": {
    "type": "git",
    "url": "https://github.com/bijanvakili/graphwalker"
  },
  "dependencies": {
    "jquery": "2.2.3",
    "lodash": "4.12.0",
    "backbone": "1.3.3",
    "backbone.typeahead": "https://github.com/mojotech/backbone.typeahead.git#1.1.0",
    "bluebird": "3.3.5",
    "svg.js": "2.6.2",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "eslint": "2.2.0",
    "eslint-config-standard": "5.1.0",
    "eslint-plugin-standard": "1.3.2",
    "eslint-plugin-promise": "1.0.8",
    "pre-commit": "1.1.2",
    "uglifyjs-webpack-plugin": "0.4.6",
    "webpack": "2.3.2",
    "webpack-dev-server": "2.4.2"
  },
  "scripts": {
    "dist": "NODE_ENV=production webpack",
    "build": "NODE_ENV=development webpack",
    "watch": "NODE_ENV=development webpack --watch --progress",
    "web": "NODE_ENV=development webpack-dev-server",
    "docker_web": "npm install && npm run web -- --host=0.0.0.0",
    "clean": "rm -rf ./build ./node_modules",
    "partial_clean": "rm -rf ./build",
    "todos": "grep -R TODO app/*",
    "lint": "eslint '**/*.js'",
    "lint-changed": "git diff --name-only --cached --relative | grep '\\.js$' | xargs ./node_modules/.bin/eslint"
  },
  "pre-commit": [
    "lint-changed"
  ]
}
